{% extends 'back/base.html.twig' %}

{% block title %}VideoRoom{% endblock %}

{% block body %}
    <h1>VideoRoom</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ video_room.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ video_room.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ video_room.description }}</td>
            </tr>
            <tr>
                <th>Link</th>
                <td>{{ video_room.link }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ video_room.createdAt ? video_room.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated_at</th>
                <td>{{ video_room.updatedAt ? video_room.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_video_room_index') }}">back to list</a>

    <a href="{{ path('admin_video_room_edit', {'id': video_room.id}) }}">edit</a>

    {{ include('back/video_room/_delete_form.html.twig') }}
